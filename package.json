{
  "name": "ihsm",
  "version": "0.0.1",
  "description": "A fully featured Hierarchical State Machine implementation in JS",
  "main": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pretest": "npm run build",
    "test": "jasmine --config=jasmine.json",
    "autotest": "nodemon --ext js -w build --exec 'jasmine --config=jasmine.json'",
    "lint": "eslint --ignore-path .gitignore src/",
    "fastbuild": "tsc",
    "build": "grunt build",
    "predoc": "del-cli ./build/doc/api",
    "doc": "typedoc ./src",
    "preexample": "tsc --build tsconfig.json",
    "example": "cd build/root && node ./examples/example.js",
    "clean": "grunt clean"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filasieno/hsmjs.git"
  },
  "keywords": [
    "hsm",
    "hierarchical",
    "fsm",
    "state machine",
    "hierarchical state machine",
    "sm"
  ],
  "author": "Fabio N. Filasieno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filasieno/hsmjs/issues"
  },
  "homepage": "https://github.com/filasieno/hsmjs#readme",
  "dependencies": {
    "@types/chai": "^4.2.10",
    "chai": "^4.2.0",
    "queue": "^6.0.1"
  },
  "devDependencies": {
    "@types/eslint": "~6.1.8",
    "@types/jasmine": "~3.5.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~13.7.7",
    "@typescript-eslint/eslint-plugin": "~2.21.0",
    "@typescript-eslint/parser": "~2.21.0",
    "del-cli": "~3.0.0",
    "eslint": "~6.8.0",
    "eslint-config-google": "~0.14.0",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jasmine": "^2.1.0",
    "grunt-ts": "^6.0.0-beta.22",
    "jasmine": "~3.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "node": "~13.9.0",
    "nodemon": "~2.0.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.6.2",
    "typedoc": "~0.16.11",
    "typescript": "~3.8"
  }
}
